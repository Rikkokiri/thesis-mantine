{
  "name": "mantine-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test:w": "vitest",
    "cy:open": "cypress open",
    "e2e:chrome": "cypress run --browser chrome",
    "cy:screenshots": "cypress run --browser chrome --spec cypress/e2e/screenshot-tests/*",
    "lint:fix": "eslint \"./**/*.{ts,tsx,js,jsx}\" --fix",
    "prettify": "prettier --write \"./**/*.{ts,tsx,js,jsx,css}\"",
    "prepare": "husky",
    "pre-commit": "lint-staged --concurrent false",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache",
    "lint:stylelint": "stylelint '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run vitest && npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@mantine/core": "^7.10.1",
    "@mantine/hooks": "^7.10.1",
    "i18next": "^23.11.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-icons": "^5.2.1",
    "react-intersection-observer": "^9.10.2",
    "react-router-dom": "^6.22.3",
    "react": "^18.2.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react-dom": "^18.2.21",
    "@types/react": "^18.2.65",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress-visual-regression": "5.0.0",
    "cypress": "^13.10.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-mantine": "3.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-react": "^7.34.0",
    "eslint": "^8.57.0",
    "globals": "^15.2.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.5",
    "postcss-preset-mantine": "1.14.4",
    "postcss-simple-vars": "^7.0.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prop-types": "^15.8.1",
    "storybook-dark-mode": "^3.0.3",
    "storybook": "^8.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint": "^16.2.1",
    "typescript": "^5.4.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vite": "^5.1.6",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css}": "prettier --write",
    "*.{ts,tsx,js,jsx}": "eslint",
    "*.{css}": "stylelint"
  }
}
